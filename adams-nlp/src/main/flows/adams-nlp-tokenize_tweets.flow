adams.flow.control.Flow
	-annotation
	Replays a tweet archive and tokenizes the tweet text using TweetNLP\'s Twokenize.
	-actor
		adams.flow.source.TweetReplay
			-replay
				adams.data.io.input.SpreadSheetConverterArchive
					-archive
					${EXAMPLE_FLOWS}/data/tweet-archive.csv
	-actor
		adams.flow.transformer.TwitterConverter
			-converter
				adams.data.twitter.TextConverter
					-separator
					\t
	-actor
		adams.flow.transformer.Tokenize
			-output-array
			true
			-tokenizer
				adams.flow.transformer.tokenizer.Twokenize
	-actor
		adams.flow.transformer.StringJoin
			-glue
			\n
	-actor
		adams.flow.sink.DisplayPanelManager
			-provider
				adams.flow.sink.Display
					-writer
						adams.data.io.output.NullWriter
	-flow-execution-listener
		adams.flow.execution.NullListener

